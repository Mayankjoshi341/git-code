i = 2
div = len(value_to_find)//2
print(div)
while div >= i:
    l1 = value_to_find[:div]
    l2 = value_to_find[div:]
    print("l1=",l1)
    print("l2=",l2)
    div = div//2
    if div >= i:
        l3 = l1[:div]
        l4 = l1[div:]
        l5 = l2[:div]
        l6 = l2[:div]
        print("l3=",l3)
        print("l4=",l4)
        print("l5=",l5)
        print("l6=",l6)
    div = div//2