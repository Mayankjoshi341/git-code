import string
code = {
    'a': '!','b': '@','c': '#','d': '$','e': '%','f': '^','g': '&','h': '*','i': '(','j': ')',
    'k': '{','l': '}','m': '^','n': '"','o': ';','p': '<','q': '>','r': ',','s': '.','t': '?',
    'u': "'",'v': ':','w': '+','x': '-','y': '_','z': '~',' ': '/','.': "`"
    }
print("hello folkes!")
print("enter the text ") 
text = str(input("==>"))
print("processing...")
print("....")
value_to_find = [*text]
print(value_to_find)
keys = [key for key , value in code.items() if value in value_to_find]
msg = ''.join(keys)
print(msg)